{
	"debug": {
		"port": 5865,
		"inspector": {
			"webPort": 9008,
			"preload": true
		}
	},
    "watch": {
        "client-src": {
            "src": [
                "shared/**/*.js"
            ]
        }
    },
    "assets": {
        "static": [
            {
                "glob": "*",
                "dest": "webfonts",
                "base": "${libDir}/bower_components/bootstrap-sass-official/assets/fonts/bootstrap"
            }
        ]
    }
}